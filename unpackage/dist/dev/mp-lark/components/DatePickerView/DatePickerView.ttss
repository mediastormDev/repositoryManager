.datepicker_view {
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
  background-color: #ccc;
  z-index: 10;
}
.datepicker_view .date_btn_view {
  display: flex;
  align-items: center;
  margin: 10px;
  background-color: #ccc;
}
.datepicker_view .date_btn_view > button {
  flex: 1;
}
.datepicker_view .picker_text {
  line-height: 50px;
  text-align: center;
  font-size: 15px;
}
