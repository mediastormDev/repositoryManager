.my_order_view{padding:10px 10px 20px;background-color:#fff;margin-top:10px}.my_order_view .order_date{font-size:14px;font-weight:400;color:#24252c}.my_order_view .order_footer{display:flex;align-items:center;justify-content:space-between;margin-top:10px}.my_order_view .order_footer .footer_title{font-size:14px;font-weight:400;color:#24252c}.my_order_view .modify_btn{border-radius:10px;border:1px solid #D8D8D8;font-size:14px;font-weight:600;color:#24252c;background-color:#fff;padding:6px 16px}.my_order_view .return_btn{border-radius:10px;padding:6px 16px;border:none;font-size:14px;font-weight:600;color:#a2662a;background-color:#ffda71}
