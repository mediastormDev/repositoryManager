.drawer_bg{position:fixed;left:0;right:0;top:0;bottom:0;background:rgba(0,0,0,.1);animation-name:opacityAn;animation-duration:.1s;z-index:9}.drawer_bg .drawer_body{background-color:#fff;padding:10px;border-radius:10px 10px 0 0;position:absolute;bottom:0;left:0;right:0;min-height:350px;transform:translateY(20%);animation-name:translateYAn;animation-duration:.1s}@keyframes translateYAn{0%{bottom:-100%}to{bottom:0px}}@keyframes opacityAn{0%{opacity:0}to{opacity:1}}
