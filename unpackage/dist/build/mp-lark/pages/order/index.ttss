.tabs_view{display:flex;align-items:center;justify-content:space-between;background-color:#fff}.tabs_view .tab{font-size:14px;font-weight:600;color:#9e9fa2;display:flex;align-items:center;justify-content:center;flex:1;padding:16px 0 15px;position:relative}.tabs_view .tab.selected{color:#24252c}.tabs_view .tab.selected:after{content:"";position:absolute;bottom:5px;left:0;right:0;margin:auto;width:18px;height:4px;background:#FC6294;border-radius:2px}
